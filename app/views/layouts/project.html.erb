<!DOCTYPE html>
<html>
<head>
  <title>Finner</title>
  <%= stylesheet_link_tag "projects", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="wrapper">
    <div id="navbar">
      <%= render 'shared/navbar' %><br /><br />
    </div>
    <div id="content">
      <div class="cols">
        <%= yield :cols %>
      </div>
    </div>
    <div id="footer">
      <%= render 'shared/footer' %>
    </div>
  </div>
</body>
</html>
