<%= "Name : " %><%= project.name %><br />
<%= "Description : " %><%= project.description %><br />
<%= "Open : " %>
<% if project.open %>
  <%= "Open" %>
<% else %>
  <%= "Private" %>
<% end %>
<br />
<%= "Icon : " %><%= image_tag project.icon.url(:thumb) %><br />
<%= "Users : " %>
<% project.users.each  do |user| %>
  <%= image_tag user.icon.url(:thumb) %>
<% end %>
<br />
